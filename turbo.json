{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"start": {
			"dependsOn": ["^build"]
		},
		"start:app": {},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	},
	"globalDependencies": [".env"],
	"globalEnv": [
		"API_KEY",
		"ENVIRONMENT",
		"TOKEN_SECRET",
		"DB_NAME",
		"AWS_BUCKET_NAME",
		"AWS_BUCKET_REGION",
		"AWS_ACCESS_KEY_ID",
		"AWS_SECRET_ACCESS_KEY",
		"FRONTEND_URL",
		"PORT",
		"NEXT_PUBLIC_ENVIRONMENT",
		"NEXT_PUBLIC_API_KEY",
		"BACKEND_URL",
		"HOST_NAME",
		"EMAIL_ACCOUNT",
		"EMAIL_PASSWORD",
		"SUBSCRIPTION_URL",
		"BUGSNAG_KEY",
		"MONGO_DB_URI",
		"FIREBASE_APP_ID",
		"FIREBASE_API_KEY",
		"FIREBASE_AUTH_DOMAIN",
		"FIREBASE_MEASUREMENT_ID",
		"FIREBASE_MESSAGING_SENDER_ID",
		"FIREBASE_PROJECT_ID",
		"FIREBASE_STORAGE_BUCKET",
		"STRIPE_SECRET_KEY",
		"STRIPE_PUBLISHABLE_KEY",
		"STRIPE_WEBHOOK_SECRET",
		"STRIPE_SECRET_KEY_BRAZIL",
		"STRIPE_PUBLISHABLE_KEY_BRAZIL",
		"STRIPE_WEBHOOK_SECRET_BRAZIL"
	]
}
